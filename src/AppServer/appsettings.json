{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "InternalTokens": {
    "Issuer": "astraid-api",
    "Audience": "astraid-app",
    "AllowedServices": [
      "api"
    ],
    "AllowedAlgorithms": [
      "RS256"
    ],
    "JwksUrl": "https://localhost:7002/internal/.well-known/jwks.json",
    "JwksRefreshMinutes": 5,
    "JwksInternalApiKey": "__REPLACE_ME__",
    "AllowLegacyHs256": false,
    "LegacyHs256Secret": "__REPLACE_ME__"
  },
  "AuthServer": {
    "Issuer": "https://localhost:7001/"
  },
  "Redis": {
    "ConnectionString": "localhost:6379,abortConnect=false"
  },
  "Hangfire": {
    "StorageConnectionString": "<set-in-environment-or-user-secrets>"
  },
  "AllowedHosts": "*",
  "AppServer": {
    "Mtls": {
      "Enabled": false,
      "RequireClientCertificate": false,
      "AllowedClientThumbprints": [
        "__REPLACE_ME__"
      ],
      "AllowedClientSubjectNames": [
        "CN=__REPLACE_ME__"
      ]
    }
  }
}
