{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "Services": {
    "Cms": {
      "BaseUrl": "https://localhost:7003"
    }
  },
  "Auth": {
    "Issuer": "https://localhost:7001/",
    "Audience": "api",
    "ClockSkewSeconds": 0
  },
  "Api": {
    "AuthServer": {
      "BaseUrl": "https://localhost:7001",
      "ApiKey": ""
    }
  },
  "InternalTokens": {
    "Issuer": "astraid-api",
    "Audience": "astraid-content",
    "LifetimeMinutes": 2,
    "SigningKey": "<set-in-environment-or-user-secrets>"
  }
}